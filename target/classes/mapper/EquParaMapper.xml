<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jan.wat.mapper.EquParaMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jan.wat.pojo.EquPara">
        <id column="ID" property="id" />
        <result column="Name" property="name" />
        <result column="ParaGroup_ID" property="paragroupId" />
        <result column="Type" property="type" />
        <result column="ReadOnly" property="readonly" />
    </resultMap>

    <select id="queryParaList" resultType="com.jan.wat.pojo.vo.EquParaQuery">
        select
            ep.ID as id,
            ep.Name as name,
            epg.Name as paraname,
            ep.Type as type,
            ep.ReadOnly as readonly
        from rdas_eng.equ_para ep, rdas_eng.equ_paragroup epg
        where ep.ParaGroup_ID = epg.ID
        order by id;
    </select>

</mapper>
